<Window x:Class="DADOMI.View.Windows.UserinfoWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DADOMI.View.Windows"
        mc:Ignorable="d"
        Title="DADOMI" Height="390" Width="350"
        ResizeMode="NoResize"
        x:Name="UserinfoW">
    <Grid DataContext="{StaticResource frameVm}">
        <Frame Source="{Binding UserinfoFrameSource}"
               NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
